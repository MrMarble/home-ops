{
  "symbols.folders.associations": {
    "cluster": "folder-blue",
    "apps": "folder-app",
    "crds": "folder-assets",
    "provision": "folder-green",
    "scripts": "folder-red-code"
  },
  "prettier.configPath": ".github/linters/.prettierrc.yaml",
  "prettier.ignorePath": ".github/linters/.prettierignore",
  "yaml.schemas": {
    "https://json.schemastore.org/kustomization.json": "kubernetes/**/kustomization.yaml",
    "kubernetes://schema/v1@namespace": "kubernetes/**/namespace.yaml",
    "https://raw.githubusercontent.com/datreeio/CRDs-catalog/main/source.toolkit.fluxcd.io/helmrepository_v1beta2.json": "kubernetes/home-cluster/flux/repositories/helm/*.yaml",
    "https://raw.githubusercontent.com/datreeio/CRDs-catalog/main/kustomize.toolkit.fluxcd.io/kustomization_v1.json": "kubernetes/**/ks.yaml",
    "https://taskfile.dev/schema.json": "**/Taskfile.yaml"
  },
  "yaml.schemaStore.enable": true,
  "conventionalCommits.scopes": [
    "vscode",
    "ci",
    "infra",
    "home",
    "docs",
    "kyverno",
    "media",
    "monitoring",
    "networking",
    "system-upgrade",
    "web-tools",
    "cluste"
  ]
}
